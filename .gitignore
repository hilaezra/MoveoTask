# Ignore node_modules in both "client" and "server" directories
/client/node_modules/
/server/node_modules/

# Ignore build artifacts in both "client" and "server" directories
/client/build/
/client/dist/
/server/build/
/server/dist/

# Ignore environment files in both "client" and "server" directories
/client/.env*
/server/.env*

# Ignore logs in both "client" and "server" directories
/client/logs/
/server/logs/

# Ignore any temporary files in both "client" and "server" directories
/client/*.log
/client/*.tmp
/server/*.log
/server/*.tmp

# Ignore macOS system files in both "client" and "server" directories
/client/.DS_Store
/server/.DS_Store

# Ignore Visual Studio Code settings in both "client" and "server" directories
/client/.vscode/
/server/.vscode/

# Ignore package-lock files in both "client" and "server" directories
/client/package-lock.json
/client/yarn.lock
/server/package-lock.json
/server/yarn.lock

# Ignore environment-specific files in both "client" and "server" directories
/client/.env.local
/client/.env.development.local
/client/.env.test.local
/client/.env.production.local
/server/.env.local
/server/.env.development.local
/server/.env.test.local
/server/.env.production.local

# Ignore files generated by create-react-app in the "client" directory
/client/coverage/
/client/build/
/client/static/